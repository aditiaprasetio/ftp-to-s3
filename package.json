    "rxjs": "^6.5.4",
    "sequelize": "6.6.2",
    "sequelize-typescript": "2.1.5",
    "swagger-ui-express": "^4.2.0",
    "ulid": "^2.3.0",
    "uuid": "^8.3.2"
