    "@nestjs/sequelize": "0.2.0",
    "@nestjs/swagger": "^5.1.5",
    "@types/bcrypt": "^3.0.0",
    "axios": "^0.26.0",
    "base-64": "^1.0.0",
    "basic-ftp": "^5.0.3",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cache-manager": "^3.4.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compare-versions": "^3.6.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "crypto-random-string": "^3.3.1",
    "dotenv": "^8.2.0",
    "express-jwt": "5.3.1",
    "fast-xml-parser": "^4.0.12",
